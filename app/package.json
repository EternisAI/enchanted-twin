{
  "name": "enchanted",
  "version": "0.0.1",
  "description": "An Electron application with React and TypeScript",
  "main": "./out/main/index.js",
  "author": "example.com",
  "homepage": "https://electron-vite.org",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint --cache .",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "codegen": "graphql-codegen --config src/renderer/src/graphql/codegen.ts",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "electron-vite build && electron-builder --mac --publish always",
    "build-local:mac": "electron-vite build && electron-builder --mac",
    "build:linux": "electron-vite build && electron-builder --linux",
    "build:dev:mac": "npm run build && electron-builder --mac --config electron-builder.dev.yml --publish always",
    "build:dev:win": "npm run build && electron-builder --win --config electron-builder.dev.yml --publish always",
    "build:dev:linux": "npm run build && electron-builder --linux --config electron-builder.dev.yml --publish always",
    "build:helper": "node scripts/build.js"
  },
  "dependencies": {
    "@apollo/client": "^3.13.7",
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^4.0.0",
    "@graphql-codegen/cli": "^5.0.5",
    "@napi-rs/image": "^1.9.2",
    "@radix-ui/react-alert-dialog": "^1.1.7",
    "@radix-ui/react-collapsible": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.7",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-scroll-area": "^1.2.6",
    "@radix-ui/react-separator": "^1.1.6",
    "@radix-ui/react-slot": "^1.2.0",
    "@radix-ui/react-switch": "^1.2.2",
    "@radix-ui/react-tabs": "^1.1.9",
    "@radix-ui/react-tooltip": "^1.2.4",
    "@react-three/drei": "^10.0.7",
    "@react-three/fiber": "^9.1.2",
    "@react-three/postprocessing": "^3.0.4",
    "@splinetool/react-spline": "^4.0.0",
    "@tailwindcss/vite": "^4.1.4",
    "@tanstack/react-router": "^1.116.0",
    "@tanstack/react-router-devtools": "^1.116.0",
    "axios": "^1.10.0",
    "babel-plugin-glsl": "^1.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "electron-log": "^5.3.3",
    "electron-store": "^8.1.0",
    "electron-updater": "^6.3.9",
    "extract-zip": "^2.0.1",
    "firebase": "^11.10.0",
    "framer-motion": "^12.7.2",
    "glslify": "^7.1.1",
    "graphql": "^16.10.0",
    "graphql-ws": "^6.0.4",
    "katex": "^0.16.22",
    "leva": "^0.10.0",
    "lottie-react": "^2.4.1",
    "lucide-react": "^0.488.0",
    "node-machine-id": "^1.1.12",
    "openai": "^5.9.0",
    "posthog-node": "^4.17.1",
    "react-focus-lock": "^2.13.6",
    "react-markdown": "^10.1.0",
    "react-qr-code": "^2.0.15",
    "rehype-highlight": "^7.0.2",
    "rehype-katex": "^7.0.1",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.1",
    "remark-math": "^6.0.0",
    "sonner": "^2.0.3",
    "split2": "^4.2.0",
    "tailwind-merge": "^3.2.0",
    "tailwindcss": "^4.1.4",
    "tar": "^7.4.3",
    "three": "^0.176.0",
    "tw-animate-css": "^1.2.5",
    "unzipper": "^0.12.3",
    "uuid": "^11.1.0",
    "wait-on": "^8.0.3",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "^3.0.0",
    "@electron-toolkit/eslint-config-ts": "^3.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@tanstack/router-plugin": "^1.116.1",
    "@types/node": "^22.14.1",
    "@types/react": "^19.1.1",
    "@types/react-dom": "^19.1.2",
    "@types/unzipper": "^0.10.11",
    "@vitejs/plugin-react": "^4.3.4",
    "electron": "^35.1.5",
    "electron-builder": "^25.1.8",
    "electron-vite": "^3.1.0",
    "eslint": "^9.24.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "prettier": "^3.5.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "rollup": "4.44.2",
    "typescript": "^5.8.3",
    "vite": "^6.2.6"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "electron",
      "esbuild"
    ]
  }
}
