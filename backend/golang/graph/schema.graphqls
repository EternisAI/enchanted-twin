type UserProfile {
  name: String
}

input UpdateProfileInput {
  name: String
}

type Query {
  profile: UserProfile!
}

type Mutation {
  updateProfile(input: UpdateProfileInput!): Boolean!
  addDataSource(input: AddDataSourceInput!): Boolean!
}

input AddDataSourceInput {
  dataSourceName: String!
  path: String!
  username: String!
}
