COMPLETIONS_API_KEY=open-router-api-key
COMPLETIONS_API_URL=https://openrouter.ai/api/v1
COMPLETIONS_MODEL=openai/gpt-4.1
REASONING_MODEL=openai/o3
EMBEDDINGS_API_KEY=openai-api-key
EMBEDDINGS_API_URL=https://api.openai.com/v1
EMBEDDINGS_MODEL=text-embedding-3-small
GRAPHQL_PORT=44999
TELEGRAM_CHAT_SERVER=https://enchanted-proxy-telegram-dev.up.railway.app/query

# `HOLON_API_URL`` is pre-release. Announced soon
HOLON_API_URL=

# `PROXY_TEE_URL` should be used only if LLM requests are proxied through the proxy. In this case `COMPLETIONS_API_KEY` and `EMBEDDINGS_API_KEY` are not needed
PROXY_TEE_URL=http://localhost:12000

# Should local embeddings model be used using onnx-engine
USE_LOCAL_EMBEDDINGS="true"

# Enchanted MCP server offers free web search and image generation, however requires authentication.
ENCHANTED_MCP_URL=""

# Anonymizer Type: "llm" (default), "no-op", "mock", or "local"
ANONYMIZER_TYPE=llm
MOCK_ANONYMIZER_DELAY=10ms
# Examples:
# ANONYMIZER_TYPE=llm      # LLM-based anonymization (default)
# ANONYMIZER_TYPE=no-op    # No anonymization
# ANONYMIZER_TYPE=mock     # Mock with predefined rules
# ANONYMIZER_TYPE=local    # Local LLM (requires model files)

# Specify database and app path
APP_DATA_PATH=""
DB_PATH=""

# PostgreSQL configuration
# POSTGRES_TIMEZONE: Set the timezone for PostgreSQL (default: UTC)
# Examples: UTC, America/New_York, Europe/London, Asia/Tokyo
# POSTGRES_TIMEZONE=UTC